  issn		= {1091-6490},
  journal	= {Proceedings of the National Academy of Sciences},
  keywords	= {Animals,Biodiversity,Biological Evolution,Diversification
		  Rates,Jaw,Jaw: anatomy \& histology,Likelihood
		  Functions,Phylogeny,Vertebrates,Vertebrates:
		  genetics,medusa},
  mendeley-tags	= {Diversification Rates,medusa},
